{
  "name": "vied",
  "productName": "Vied",
  "version": "1.0.0",
  "description": "Desktop video editor for trimming and exporting videos",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build": "electron-builder",
    "build:mac": "electron-builder --mac",
    "build:mac:dmg": "electron-builder --mac --dmg"
  },
  "keywords": [],
  "author": {
    "name": "Aleksandr Gaun",
    "email": "17973759+Rhoahndur@users.noreply.github.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/plugin-webpack": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "babel-loader": "^10.0.0",
    "css-loader": "^6.11.0",
    "electron": "38.4.0",
    "electron-builder": "^26.0.12",
    "node-loader": "^2.1.0",
    "style-loader": "^3.3.4"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-squirrel-startup": "^1.0.1",
    "fluent-ffmpeg": "^2.1.3",
    "lucide-react": "^0.548.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-resizable-panels": "^3.0.6",
    "tailwind-merge": "^3.3.1"
  },
  "build": {
    "appId": "com.vied.app",
    "productName": "Vied",
    "mac": {
      "category": "public.app-category.video",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "files": [
      ".webpack/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "dmg": {
      "title": "Vied Installer",
      "window": {
        "width": 600,
        "height": 400
      },
      "contents": [
        {
          "x": 150,
          "y": 200,
          "type": "file"
        },
        {
          "x": 450,
          "y": 200,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  }
}
